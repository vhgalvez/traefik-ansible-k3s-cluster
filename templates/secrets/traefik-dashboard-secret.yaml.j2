# templates\secrets\traefik-dashboard-secret.yaml.j2
apiVersion: v1
kind: Secret
metadata:
  name: {{ traefik_dashboard_secret_name | default('traefik-dashboard-secret') }}
  namespace: {{ traefik_namespace | default('kube-system') }}
  annotations:
    sealedsecrets.bitnami.com/managed: "true"
type: Opaque
stringData:
  users: "{{ traefik_basic_auth }}"